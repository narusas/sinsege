<script>
function law_pop(filen)
{
	var style="top=0,left=0,width=663,height=750,scrollbars=yes,resizable=yes,border=0";
	var filename = "law_popup.php?filename="+filen;
	lawfrm = window.open(filename,'law_pop',style);
	lawfrm.focus();
}
function cgs_pop(filename,tg)
{
	var form = document.cgs_frm;
	
	var style="top=0,left=0,width=645,height=650,scrollbars=yes,resizable=yes,border=0";
	var csgfrm = "";
	csgfrm = window.open("about:blank",tg,style);
	form.action=filename;	
	form.target=tg;
	form.submit();
	csgfrm.focus();
}
</script>
