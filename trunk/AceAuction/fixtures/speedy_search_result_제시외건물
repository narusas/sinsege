function frm_one_del_proc(serino)
{
	a=confirm('정말로 해제 하시겠습니까?');	
	
	if(a)
	{
		document.info_list2.serino.value = serino;
		document.info_list2.action = 'interest_event_del.php';
		document.info_list2.submit();
	}
}
